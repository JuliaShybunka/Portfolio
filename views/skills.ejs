<section class="skills" id='skills'>
    <div class="container-fluid">
        <h3 class="skills-title title">Skills</h3>
        <div class="container">
            <div class="row">
                <% skills.forEach(skill=>{%>
                    <div class="col-md-6">
                        <h5 class="d-inline">
                            <%= skill.title %>
                        </h5>
                        <span>1</span>
                        <span>2</span>
                        <span>3</span>
                        <span>4</span>
                        <span>5</span>
                        <p>
                            <%= skill.number %>
                        </p>
                    </div>
                    <% })  %>
            </div>
        </div>
    </div>
</section>